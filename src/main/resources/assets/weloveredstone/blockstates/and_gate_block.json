{
  "multipart": [
    {
      "when": { "facing": "north", "powered": false },
      "apply": { "model": "weloveredstone:block/and_gate_block" }
    },
    {
      "when": { "facing": "north", "powered": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_powered" }
    },
    {
      "when": { "facing": "north", "mid": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid" }
    },
    {
      "when": { "facing": "north", "mid": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid_powered" }
    },
    {
      "when": { "facing": "north", "left": false, "right": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides" }
    },
    {
      "when": {
        "OR": [
          { "facing": "north", "left": true },
          { "facing": "north", "right": true }
        ]
      },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides_powered" }
    },

    {
      "when": { "facing": "east", "powered": false },
      "apply": { "model": "weloveredstone:block/and_gate_block", "y": 90 }
    },
    {
      "when": { "facing": "east", "powered": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_powered", "y": 90 }
    },
    {
      "when": { "facing": "east", "mid": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid", "y": 90 }
    },
    {
      "when": { "facing": "east", "mid": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid_powered", "y": 90 }
    },
    {
      "when": { "facing": "east", "left": false, "right": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides", "y": 90 }
    },
    {
      "when": {
        "OR": [
          { "facing": "east", "left": true },
          { "facing": "east", "right": true }
        ]
      },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides_powered", "y": 90 }
    },

    {
      "when": { "facing": "south", "powered": false },
      "apply": { "model": "weloveredstone:block/and_gate_block", "y": 180 }
    },
    {
      "when": { "facing": "south", "powered": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_powered", "y": 180 }
    },
    {
      "when": { "facing": "south", "mid": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid", "y": 180 }
    },
    {
      "when": { "facing": "south", "mid": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid_powered", "y": 180 }
    },
    {
      "when": { "facing": "south", "left": false, "right": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides", "y": 180 }
    },
    {
      "when": {
        "OR": [
          { "facing": "south", "left": true },
          { "facing": "south", "right": true }
        ]
      },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides_powered", "y": 180 }
    },

    {
      "when": { "facing": "west", "powered": false },
      "apply": { "model": "weloveredstone:block/and_gate_block", "y": 270 }
    },
    {
      "when": { "facing": "west", "powered": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_powered", "y": 270 }
    },
    {
      "when": { "facing": "west", "mid": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid", "y": 270 }
    },
    {
      "when": { "facing": "west", "mid": true },
      "apply": { "model": "weloveredstone:block/and_gate_block_mid_powered", "y": 270 }
    },
    {
      "when": { "facing": "west", "left": false, "right": false },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides", "y": 270 }
    },
    {
      "when": {
        "OR": [
          { "facing": "west", "left": true },
          { "facing": "west", "right": true }
        ]
      },
      "apply": { "model": "weloveredstone:block/and_gate_block_sides_powered", "y": 270 }
    }
  ]
}
